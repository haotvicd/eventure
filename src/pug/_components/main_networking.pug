  #modalEditTable.modalGen.modalGen--sponsor.modal.fade(tabindex='-1' role='dialog' aria-labelledby='addGroupModalTitle' aria-hidden='true')
    .modal-dialog.modal-dialog-centered(role='document')
      form.modal-content
        .modal_head
          h2.ttl-primary Edit Table
          button(type='button' class="btn btn-circle trans" data-dismiss="modal")
            img(src=path + "/assets/images/ico_close_white.svg", alt="")
        .modal-wrapper
          .modal_body
            .modal_body-col.modal_body-col--width100
              .m-input.m-input_counter.js-counter
                span.m-input_label Table name
                  small.warning *
                .input-area
                  input.input.input-text(type="text" maxlength="200")
                  .counter
                    span.count-up 0
                    |/
                    span.count-max 200
              .m-input.treeSelector
                span.m-input_label Tags
                  button.btn-tooltip(type='button' data-toggle='tooltip' data-placement='top' title='Tooltip on top')
                a.btn-clear-select(href="javascript:;")
                  img(src=path+"/assets/images/ico_clear.svg", alt="")
                .form-control.branch.input(data-value="" data-placeholder="Enter tags") Enter tags
                .custom-select-list
                  .input-a
                    .input-a_inner
                      input.input.input-text.input-search(data-search type="text" placeholder="Enter tags")
                      a.btn-clear.trans(href="javascript:;") ⌘&nbsp;F&nbsp;
                  .wrap-select
                    ul.l-multiple
                      li(data-filter-item data-filter-name="tag 1")
                        label.module-check
                          input.form-checkbox(type="checkbox", name="tag[]" value="Tag 1")
                          span.label
                            span Tag 1
                      li(data-filter-item data-filter-name="tag 2")
                        label.module-check
                          input.form-checkbox(type="checkbox", name="tag[]" value="Tag 2")
                          span.label
                            span Tag 2
                      li(data-filter-item data-filter-name="tag 3")
                        label.module-check
                          input.form-checkbox(type="checkbox", name="tag[]" value="Tag 3")
                          span.label
                            span Tag 3
                      li(data-filter-item data-filter-name="tag 4")
                        label.module-check
                          input.form-checkbox(type="checkbox", name="tag[]" value="Tag 4")
                          span.label
                            span Tag 4
              .m-input.treeSelector
                span.m-input_label Host
                  small.warning *
                  button.btn-tooltip(type='button' data-toggle='tooltip' data-placement='top' title='Tooltip on top')
                a.btn-clear-select(href="javascript:;")
                  img(src=path+"/assets/images/ico_clear.svg", alt="")
                .form-control.branch.input(data-value="" data-placeholder="Enter host") Enter host
                .custom-select-list
                  .input-a
