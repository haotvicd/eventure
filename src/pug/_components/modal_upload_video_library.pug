#uploadVideoFromLibrary.modal.fade(tabindex='-1' role='dialog' aria-labelledby='uploadVideo' aria-hidden='true')
  .modal-dialog.modal-dialog-centered(role='document')
    .modal-content
      .modal_head
        .ttl-primary.sm Add from video library
        button(type='button' class="btn btn-circle trans" data-dismiss="modal")
          img(src=path + "/assets/images/ico_close_white.svg", alt="")
      .modal-wrapper
        .modal_body
          .m-input
            .input-area.area-select.js-select.video-select
              span.label.trigger Select video from library
              input(type='text', value='Name-video-upload.mp4' placeholder='')
              .box-select
                .input-a
                  .input-a_inner
                    input.input.input-text.input-search(data-search='' type='text' placeholder='Search session')
                ul.l-select
                  li(data-value='All our PRO level features at your finge...' data-filter-name='All our PRO level features at your finge...')
                    label.module-check
                      input.form-checkbox(type='radio' name='video' value='All our PRO level features at your finge...')
                      span.label
                        span.image
                          img(src=path+'/assets/images/video01.png' alt='What describes you best?')
                        span
                          | All our PRO level features at your finge...
                          small 00:12:00
                  li(data-value='Melts in your mouth, not in...' data-filter-name='Melts in your mouth, not in...')
                    label.module-check
                      input.form-checkbox(type='radio' name='video' value='Melts in your mouth, not in...')
                      span.label
                        span.image
                          img(src=path+'/assets/images/video02.png' alt='What describes you best?')
                        span
                          | Melts in your mouth, not in...
                          small 00:03:07
                  li(data-value='I accept the  Terms of use ...' data-filter-name='I accept the  Terms of use ...')
                    label.module-check
                      input.form-checkbox(type='radio' name='video' value='I accept the  Terms of use ...')
                      span.label
                        span.image
                          img(src=path+'/assets/images/video03.png' alt='What describes you best?')
                        span
                          | I accept the  Terms of use ...
                          small 00:04:23
                  li(data-value='You can change or cancel...' data-filter-name='You can change or cancel...')
                    label.module-check
                      input.form-checkbox(type='radio' name='video' value='You can change or cancel...')
                      span.label
                        span.image
                          img(src=path+'/assets/images/video04.png' alt='You can change or cancel...')
                        span
                          | You can change or cancel...
                          small 00:12:00

          video#player2.js-video(width='100%' height='288' preload='none' style='max-width: 100%; border-radius: 16px;' poster=path+'/assets/images/img_video_ol.jpg' controls='' playsinline='' webkit-playsinline='')
            source(src='//github.com/mediaelement/mediaelement-files/blob/master/big_buck_bunny.mp4?raw=true' type='video/mp4')
            track(src='dist/mediaelement.vtt' srclang='en' label='English' kind='subtitles' type='text/vtt')
        .modal_foot
          button(type="submit", class="btn btn-primary trans w-100")
            span Add a video