    .m-formBuilder_field-add
      .m-formBuilder_field-add--standard
        .m-formBuilder_field-add--title Standard Fields
        ul.m-formBuilder_field-add--list
          li(
            data-type='fullname_type'
            draggable='true'
            class='field_name'
          ) Name
          li(
            data-type='email_type'
            draggable='true'
            class='field_email'
          ) Email
          li(
            data-type='job_title_type'
            draggable='true'
            class='field_job_title'
          ) Job title
          li(
            data-type='organization_type'
            draggable='true'
            class='field_organization'
          ) Organization
          li(
            data-type='country_type'
            draggable='true'
            class='field_country'
          ) Country
          li(
            data-type='province_city_type'
            draggable='true'
            class='field_province_city'
          ) Province/ City
          li(
            data-type='upload_avatar_type'
            draggable='true'
            class='field_upload_avatar'
          ) Upload avatar
      .m-formBuilder_field-add--custom
        .m-formBuilder_field-add--title Custom Fields
        ul.m-formBuilder_field-add--list
          li(
            data-type='line_text_type'
            draggable='true'
            class='field_line_text'
          ) Line text
          li(
            data-type='paragraph_type'
            draggable='true'
            class='field_paragraph'
          ) Paragraph
          li(
            data-type='dropdown_type'
            draggable='true'
            class='field_dropdown'
          ) Dropdown
          li(
            data-type='multi_choice_type'
            draggable='true'
            class='field_multi_choice'
          ) Multi Choice
          li(
            data-type='checkboxes_type'
            draggable='true'
            class='field_checkbox'
          ) Checkboxes
          li(
            data-type='number_type'
            draggable='true'
            class='field_number'
          ) Number
