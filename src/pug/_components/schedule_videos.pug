.m-content_head
  div
    h2.title-tab Pre-Recorded Videos
    span.description-tab You can upload videos which can be played during a Live session.
    .m-content_head-button
      button.btn.btn-primary.btn-cloud_upload.trans.btn-sm(
        data-toggle="modal"
        data-target="#uploadVideo"
      )
        span Upload
      span.txt Or
      button.btn.btn-black.btn-add--library.trans.btn-sm(
        data-toggle="modal"
        data-target="#uploadVideoFromLibrary"
      )
        span Add new from library

