        | Previous
    li.page-item
      a.pages.trans(href="#") 1
    li.page-item
      span.current.pages 2
    li.page-item
      a.pages.trans(href="#") 3
    li.page-item
      a.dots.trans(href="#") ...
    li.page-item
      a.pages.trans(href="#") 22
    li.page-item
      a.pages.next.trans(href="#") Next
