      ul.nav.nav-tabs
        li.nav-link
          a.active(href="#tab1" data-toggle="tab") General
        li.nav-link
          a(href="#tab2" data-toggle="tab") Agenda
        li.nav-link
          a(href="#tab3" data-toggle="tab") Registrations
        li.nav-link
