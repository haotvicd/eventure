.m-head-event
  .wrapper
    a.btn-back.trans(href="#") BACK
    .m-head-event_inner
      .m-head-event_info
        .m-head-event_title
          h2.title Event-led Growth Summit
          span.tag Upcoming
        .m-head-event_time Start 19 Aug 2022, 11:00 - End 22 Aug 2022, 17:00
      ul.m-head-event_control
        li
          a.btn.btn-black.btn-share.trans(href="#")
            span Share
        li
          a.btn.btn-primary.btn-start.trans(href="#" data-toggle="modal" data-target="#modalStartStop")
            span Start Event
        li
          a.btn.btn-secondary.btn-close.trans(href="#" data-toggle="modal" data-target="#modalCloseEvent")
            span Close Event
