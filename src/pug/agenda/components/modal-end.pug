#modalEnd.modal.modal-confirm.fade(tabindex='-1' role='dialog' aria-labelledby='endVideoConfirm' aria-hidden='true')
    .modal-dialog.modal-dialog-centered(role='document')
      form.modal-content
        .modal_head
          h2.title This video has ended
        .modal-wrapper
          .modal_body
            .modal_body_descriptions Do you want to start another one?
          .modal_foot
            button(type="button" class="btn btn-black trans" data-dismiss="modal")
              span No
            button(type="button" class="btn btn-primary trans")
              span Yes