    .profile_cover
      img(src=path + "/assets/images/profile/cover.jpg", alt="")
      button(type='button' class='btn btn-black btn-edit-big btn-sm trans')
        small.icon
          svg(width='25' height='24' viewBox='0 0 25 24' fill='none' xmlns='http://www.w3.org/2000/svg')
            path(d='M4.48573 21.1909L9.78903 20.1302C9.98262 20.0915 10.1604 19.9964 10.3 19.8568L16.9498 13.2069C17.3403 12.8164 17.3403 12.1832 16.9498 11.7927L12.7072 7.55006C12.3167 7.15954 11.6835 7.15954 11.293 7.55006L4.64317 14.1999C4.50357 14.3395 4.40842 14.5173 4.3697 14.7109L3.30904 20.0142C3.16909 20.7139 3.78601 21.3308 4.48573 21.1909Z' fill='#fff')
            path(d='M20.6925 5.2216L19.2783 3.80739C18.1068 2.63582 16.2073 2.63582 15.0357 3.80739L14.0504 4.79268C13.6599 5.1832 13.6599 5.81637 14.0504 6.20689L18.293 10.4495C18.6836 10.8401 19.3167 10.8401 19.7073 10.4495L20.6925 9.46425C21.8641 8.29267 21.8641 6.39318 20.6925 5.2216Z' fill='#fff')


        span Edit cover
    .wrapper
      .profile_content
        .profile_content-head
          .profile_avatar
            .profile_avatar_head
              .profile_avatar-thumb
                img(src=path + "/assets/images/profile/avatar.jpg", alt="")
              a.pc-only(href="#" class='profile_avatar-update') Update avatar
              a.sp-only(href="#" class='profile_avatar-update') Update
            .profile_avatar_body
              .profile_avatar-name Eleanor Pena
              ul.profile_avatar-sns
                li
                  a(href="#" class='trans')
                    img(src=path+"/assets/images/profile/ico_twitter.svg", alt="")
                li
                  a(href="#" class='trans')
                    img(src=path+"/assets/images/profile/ico_instagram.svg", alt="")
                li
                  a(href="#" class='trans')
                    img(src=path+"/assets/images/profile/ico_facebook.svg", alt="")
              .profile_avatar-createdAt Member since Mar 15, 2022

          .profile_about
