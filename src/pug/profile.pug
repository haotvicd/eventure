                .m-EventsTable_td.col2.pc-only
                  span.txt_label.completed Completed
                .m-EventsTable_td.col3
                  button(type='button' class='btn trans btn-sm btn-primary btn-replay')
                    span Replay

            .m-dataTable_empty(style='display: none')
              h3 Nothing to see here!
              p You haven't been part of any events yet. Explore and be part of some of our upcoming events.
              button(type='button' class='btn btn-primary btn-sm trans')
                span Explore events

  include _components/modal_edit_profile



